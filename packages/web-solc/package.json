    "@vitest/coverage-v8": "3.2.4",
    "playwright": "^1.53.2",
    "tsx": "^4.20.3",
    "typescript": "^5.5.3",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "semver": "^7.6.2"
